<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
            android:opacity="opaque">
    <item android:drawable="@color/colorWhite"/>
    <item
        android:id="@+id/launch_screens_zhiye_logo"
        android:bottom="@dimen/launch_screens_zhiye_logo_bottom">
        <bitmap
            android:gravity="center"
            android:src="@drawable/zhiye_logo_144"/>
    </item>
    <item
        android:id="@+id/launch_screens_logo_text"
        android:bottom="@dimen/launch_screens_zhiye_name_logo_bottom">
        <bitmap
            android:gravity="bottom"
            android:src="@drawable/zhiye_name_logo_212x44"/>
    </item>
</layer-list>